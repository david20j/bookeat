@model IEnumerable<Proyecto_net.Models.Reserva>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Fecha)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Notas)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdlocalNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdmesaNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdusuarioNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Fecha)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Notas)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdlocalNavigation.Idlocal)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdmesaNavigation.Idmesa)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdusuarioNavigation.Idusuario)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Idreserva">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Idreserva">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Idreserva">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
