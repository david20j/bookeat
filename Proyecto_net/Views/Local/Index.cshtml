                @Html.DisplayFor(modelItem => item.Direccion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CPostal)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Coordenadas)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Telefono)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Notas)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Categoria)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Distrito)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Barrio)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Idlocal">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Idlocal">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Idlocal">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
